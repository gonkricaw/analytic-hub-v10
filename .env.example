APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=pusher
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# Pusher Configuration
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Security Configuration
# HTTPS Enforcement
HTTPS_ENFORCE=true
HTTPS_REDIRECT_STATUS=301
HTTPS_TRUST_PROXIES=false
HSTS_MAX_AGE=31536000
HSTS_INCLUDE_SUBDOMAINS=true
HSTS_PRELOAD=true

# Security Headers
X_FRAME_OPTIONS=DENY
X_CONTENT_TYPE_OPTIONS=nosniff
X_XSS_PROTECTION="1; mode=block"
REFERRER_POLICY=strict-origin-when-cross-origin
PERMISSIONS_POLICY="geolocation=(), microphone=(), camera=()"
COEP=require-corp
COOP=same-origin
CORP=same-origin
REMOVE_SERVER_HEADER=true

# Content Security Policy
CSP_ENABLED=true
CSP_REPORT_ONLY=false
CSP_REPORT_URI=
CSP_UPGRADE_INSECURE=true

# SQL Injection Prevention
SQL_INJECTION_PROTECTION=true
SQL_INJECTION_STRICT=false
SQL_INJECTION_LOG=true
SQL_INJECTION_BLOCK=true
SQL_INJECTION_MAX_VIOLATIONS=5

# Rate Limiting
RATE_LIMITING_ENABLED=true
RATE_LIMIT_MAX_ATTEMPTS=60
RATE_LIMIT_DECAY_MINUTES=1
RATE_LIMIT_SKIP_SUCCESS=false
LOGIN_RATE_LIMIT=5
LOGIN_RATE_DECAY=15
PASSWORD_RESET_RATE_LIMIT=3
PASSWORD_RESET_RATE_DECAY=60
API_RATE_LIMIT=1000
API_RATE_DECAY=60

# IP Security
IP_BLACKLIST_ENABLED=true
IP_WHITELIST_ENABLED=false
AUTO_BLACKLIST_ENABLED=true
MAX_FAILED_LOGIN_ATTEMPTS=5
BLACKLIST_DURATION_HOURS=24
GEO_BLOCKING_ENABLED=false
ALLOWED_COUNTRIES=
BLOCKED_COUNTRIES=

# Session Security
SESSION_REGENERATE_LOGIN=true
SESSION_REGENERATE_PRIVILEGE=true
SESSION_TIMEOUT_WARNING=5
CONCURRENT_SESSIONS_LIMIT=3
SESSION_TRACK_USER_AGENT=true
SESSION_TRACK_IP=true
SESSION_FINGERPRINTING=true

# Audit Logging
AUDIT_LOGGING_ENABLED=true
AUDIT_LOG_ALL_REQUESTS=false
AUDIT_LOG_FAILED_REQUESTS=true
AUDIT_LOG_SENSITIVE_OPS=true
AUDIT_RETENTION_DAYS=365
AUDIT_DATABASE_ENABLED=true
AUDIT_TABLE=activity_logs
AUDIT_DB_CONNECTION=
AUDIT_FILE_ENABLED=true
AUDIT_LOG_CHANNEL=audit
AUDIT_LOG_LEVEL=info
AUDIT_EXTERNAL_ENABLED=false
AUDIT_EXTERNAL_ENDPOINT=
AUDIT_EXTERNAL_API_KEY=

# Content Security
CONTENT_URL_ENCRYPTION=true
CONTENT_ENCRYPTION_ALGO=AES-256-CBC
CONTENT_ENCRYPTION_KEY=
CONTENT_ONE_TIME_TOKENS=true
CONTENT_TOKEN_EXPIRY=60
CONTENT_TOKEN_MAX_USES=1
IFRAME_SECURITY_ENABLED=true
IFRAME_ALLOWED_DOMAINS=
IFRAME_SANDBOX="allow-scripts allow-same-origin"

# Password Security
PASSWORD_MIN_LENGTH=8
PASSWORD_REQUIRE_UPPERCASE=true
PASSWORD_REQUIRE_LOWERCASE=true
PASSWORD_REQUIRE_NUMBERS=true
PASSWORD_REQUIRE_SYMBOLS=true
PASSWORD_HISTORY_LIMIT=5
PASSWORD_EXPIRY_DAYS=90
PASSWORD_WARNING_DAYS=7
PASSWORD_CHECK_COMMON=true
PASSWORD_CHECK_PERSONAL=true

# Two-Factor Authentication
TWO_FACTOR_ENABLED=false
TWO_FACTOR_REQUIRED_ADMIN=true
TWO_FACTOR_BACKUP_CODES=8
TWO_FACTOR_TOTP_WINDOW=1
TWO_FACTOR_REMEMBER_DAYS=30

# File Upload Security
FILE_UPLOAD_ENABLED=true
FILE_UPLOAD_MAX_SIZE=10
FILE_UPLOAD_ALLOWED_EXT="jpg,jpeg,png,gif,pdf,doc,docx,xls,xlsx"
FILE_UPLOAD_ALLOWED_MIME="image/jpeg,image/png,image/gif,application/pdf"
FILE_UPLOAD_VIRUS_SCAN=false
FILE_UPLOAD_QUARANTINE=true

# API Security
API_RATE_LIMIT_PER_MINUTE=60
API_REQUIRE_KEY=true
API_KEY_HEADER=X-API-Key
API_CORS_ENABLED=true
API_ALLOWED_ORIGINS=*

# Logging Configuration
AUDIT_LOG_RETENTION_DAYS=365
SECURITY_LOG_LEVEL=warning
SECURITY_LOG_RETENTION_DAYS=365
